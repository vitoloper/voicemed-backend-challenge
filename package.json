    "fastify": "^3.27.4",
    "fastify-swagger": "^5.1.0",
    "mongoose": "^6.2.8"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.11.0",
    "mocha": "^9.2.2",
